{
  "name": "deliver-upgrade-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "echo building for local & ng build",
    "build:test": "echo building for test env & ng build --c=test",
    "build:staging": "echo building for staging env & ng build --c=staging",
    "build:preprod": "echo building for preprod env & ng build --c=preprod",
    "build:prod": "echo building for prod & ng build --c=production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:report": "ng test --source-map=false --browsers=ChromeHeadless --watch=false --code-coverage=true",
    "sonar": "sonar-scanner",
    "prettier:check": "prettier --check \"src/**/*.{ts,css,scss,html}\"",
    "prettier": "npx prettier --write . --check \"src/**/*.{ts,css,scss,html}\"",
    "prepare": "husky"
  },
  "private": true,
  "lint-staged": {
    "src/**/*.{ts,scss,html,css,scss,json}": "prettier --write .",
    "*.json": "prettier --write ."
  },
  "dependencies": {
    "@angular/animations": "^17.3.10",
    "@angular/cdk": "^17.0.0",
    "@angular/common": "^17.3.10",
    "@angular/compiler": "^17.3.10",
    "@angular/core": "^17.3.10",
    "@angular/forms": "^17.3.10",
    "@angular/platform-browser": "^17.3.10",
    "@angular/platform-browser-dynamic": "^17.3.10",
    "@angular/router": "^17.3.10",
    "bootstrap": "^5.3.3",
    "ngx-bootstrap": "^12.0.0",
    "ngx-infinite-scroll": "^17.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.6"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.8",
    "@angular/cli": "^17.3.8",
    "@angular/compiler-cli": "^17.3.10",
    "@types/jasmine": "~4.3.0",
    "husky": "^9.0.11",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "lint-staged": "^15.2.2",
    "prettier": "3.2.5",
    "sonar-scanner": "^3.1.0",
    "typescript": "~5.4.5"
  }
}
