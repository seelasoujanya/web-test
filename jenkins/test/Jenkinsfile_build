pipeline {
  agent none
  stages {
    stage('Clean') {
      agent { label 'bmg-supplychain-deliver-upgrade-test-wrk' }
      steps {
        cleanWs()
      }
    }
    stage('Docker build and push'){
      agent { label 'bmg-supplychain-deliver-upgrade-test-wrk' }
      steps{
        // Run build script
        sh "chmod +x ./scripts/build_gcp.sh"
        sh "./scripts/build_gcp.sh test"
      }
    }
  }
}